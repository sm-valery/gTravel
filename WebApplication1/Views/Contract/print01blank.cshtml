@functions{

    string get_instableclass(bool lastrecord)
    {
        return (lastrecord) ? "td-bottom-fat" : "";
    }
}
@model gTravel.Models.Contract
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewPdf</title>
    <style>
        body {
            font-family: Arial;
            width: 1000px;
            padding-top:28px;

        }


        .blank-separator
        {
            display:block;
            height:87px;
        }


        .img-tit {
            width: 150px;
        }

        .stamp {
            width: 20%;
            position: absolute;
            z-index: -1000;
        }

        .polis-separator {
            background-color: darkslateblue;
            height: 10px;
            font-size: 10px;
            text-align: center;
            color: white;
        }

        .polis-caption {
            padding: 10px;
            text-align: center;
            font-size: 18px;
        }

        .polis-small {
            font-size: 8px;
        }

        .polis-num {
            font-size: 24px;
            white-space: nowrap;
        }

        .table-polis {
            font-size: 12px;
            border-collapse: collapse;
            width: 100%;
        }

            .table-polis th {
                vertical-align: text-top;
                padding: 5px;
            }

            .table-polis td {
                padding: 2px;
            }

            .table-polis .data {
                white-space: nowrap;
                padding-left: 1px;
                padding-right: 1px;
            }

            .table-polis .rdata {
                padding-top: 5px;
                padding-bottom: 8px;
            }

        .td-left-fat {
            border-left: solid 3px dimgray;
        }

        .td-top-fat {
            border-top: solid 3px dimgray;
        }

        .td-bottom-fat {
            border-bottom: solid 3px dimgray;
        }

        .td-right-fat {
            border-right: solid 3px dimgray;
        }

        .td-top-thin {
            border-top: solid thin dimgray;
        }

        .td-bottom-thin {
            border-bottom: solid thin dimgray;
        }

        .td-right-thin {
            border-right: solid thin dimgray;
        }

        .td-left-thin {
            border-left: solid thin dimgray;
        }

        .td-big {
            height: 40px;
        }

        .img-polis-forwhom {
            /* width: 25px;
           padding-left: 5px;*/
        }


        .text-m {
            font-size: 10px;
        }

        .text-footer {
            font-size: 12px;
        }

        .container {
            width: 1000px;
            margin: 0 auto;
        }

            .container .rlabel {
                font-weight: bold;
            }

            .container .elabel {
                font-weight: normal;
                font-size: 8px;
            }

            .container .etext {
                margin-top: 3px;
            }


            .container .col_left {
                float: left;
                width: 40px;
                height: 285px;
                padding-top: 20px;
                padding-left: 5px;
               
            }

            .container .col_center {
                float: left;
                margin-left: 10px;
                width: 600px;
            }

            .container .col_right {
                float: left;
                width: 320px;
                margin-left: 3px;
            }

        .clear {
            clear: both;
        }

        .memo {
            font-size: 10px;
        }

            .memo .cap {
                font-size: 14px;
                text-align: center;
            }

            .memo table {
                font-size: 10px;
            }

            .memo .pp {
                padding-left: 20px;
            }

        article {
            -webkit-column-count: 2;
            -moz-column-count: 2;
            column-count: 2;
            font-size: 10px;
        }
    </style>
</head>
@{
    int isb = 0;
    int maxrows = 9;

    ViewData["maxrows"] = maxrows;
}
<body>
    @{
        var risk = Model.ContractRisks.FirstOrDefault();

        string ins_sum_str = string.Format("{0:N} {1}", risk.InsSum, Model.Currency.code);


    }

    @for (int i = 0; i < 3; i++)
    {
        isb = 0;


        <div style="padding-left:680px; margin-bottom:10px;" class="polis-num">
            @Model.seria.Code
        </div>

        <div style="padding-left:300px;" class="polis-small">
            Настоящий полис  выдан на основании Правил страхования непредвиденных расходов граждан на время путешествий, утвержденных   ООО «СК «ТИТ» от 01.07.2014г.
        </div>


        <div class="container">
            <div class="col_left">
                &nbsp;
            </div>

            @{
        Html.RenderPartial("_print01", Model);
            }


        </div>

        <div style="padding-left:50px;" class="clear polis-small">
            <table class="table-polis">
                <tr>
                    <td class="polis-small" style="width:55%">
                        В соответствии с Федеральным законом «О персональных данных» от

                        27.07.2006 г. № 152-ФЗ предоставляю согласие на обработку

                        Страховщиком персональных данных на срок действия договора и в

                        течении 5(пяти) лет с даты его прекращения.  С Правилами страхования

                        ознакомлен, согласен и экземпляр  Правил страхования получил. С

                        использованием Страховщиком факсимильной печати  и

                        факсимильной подписи его представителя

                        согласен__________(подпись
                    </td>
                    <td class="polis-small" style="white-space:nowrap;">
                        __________________________
                        <br />(Подпись страхователя)
                    </td>
                    <td>
                        Полис выдан
                        <div><u>@ViewBag.agentname</u></div>
                    </td>
                    <td>
                        <img style="width:70px;" src="data:image/png;base64," />
                    </td>
                </tr>
            </table>
        </div>

        if (i < 2)
        {
            <div class="blank-separator">
                &nbsp;
            </div>
        }
    }

    @if (Model.Subjects.Count() > maxrows)
    {
        @Html.Partial("_print_inslist", Model)
    }
</body>
</html>

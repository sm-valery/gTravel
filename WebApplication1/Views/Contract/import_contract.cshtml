<ul>
    @foreach (var item in (List<gTravel.Models.import_settings>) ViewBag.settings)
    {
        <li>@item.numcol : @item.colname</li>
    }
 
</ul>


<table class="table table-striped">
    <tr>
        <th>Дата импорта</th>
        <th>Количество договоров</th>
        <th>Пользователь</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.dateinsert
            </td>
            <td>
                @item.contractcount
            </td>
            <td>
                @item.UserName
            </td>
        </tr>
    }
</table>